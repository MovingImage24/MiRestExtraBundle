{
  "name": "mi/rest-extra-bundle",
  "description": "Helper to avoid annotations with FOSRestBundle and SensioFrameworkExtraBundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4 || ^8.0",
    "friendsofsymfony/rest-bundle": "^3.0",
    "sensio/framework-extra-bundle": "^6.1"
  },
  "require-dev": {
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "phpunit/phpunit": "^9.0",
    "symfony/framework-bundle": "^5.2",
    "symfony/validator": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Bundle\\RestExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Bundle\\RestExtraBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
