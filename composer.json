{
  "name": "mi/rest-extra-bundle",
  "description": "Helper to avoid annotations with FOSRestBundle and SensioFrameworkExtraBundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "friendsofsymfony/rest-bundle": "^1.7",
    "sensio/framework-extra-bundle": "^3.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "~2.7",
    "phpunit/phpunit": "^4.8",
    "symfony/validator": "^2.7",
    "matthiasnoback/symfony-dependency-injection-test": "^0.7.4"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Bundle\\RestExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Bundle\\RestExtraBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
